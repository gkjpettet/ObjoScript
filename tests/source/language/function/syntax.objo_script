// Single expression body.
function f1() { System.print("ok") }
f1()// expect: ok

// Curly body.
function f2() {
  System.print("ok")
}
f2() // expect: ok

// Multiple statements.
function f3() {
  System.print("1")
  System.print("2")
}
// expect: 1
// expect: 2
f3()

// Extra newlines.
function f4() {


  System.print("1")


  System.print("2")


}
// expect: 1
// expect: 2
f4()