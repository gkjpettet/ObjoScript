var i = 0
while true {
  print "outer " + i
  if i > 1 then exit

  var j = 0
  while true {
	print "inner " + j
	if j > 1 then exit

	j = j + 1
  }

  i = i + 1
}

// expect: outer 0
// expect: inner 0
// expect: inner 1
