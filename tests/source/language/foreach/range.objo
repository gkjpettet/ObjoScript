foreach i in 1...5 { System.print(i) }
// Expect: 1, 2, 3, 4, 5
System.print("-")

foreach i in 1..<5 { System.print(i) }
// Expect: 1, 2, 3, 4
System.print("-")

foreach i in 5...1 { System.print(i) }
// Expect: 5, 4, 3, 2, 1
System.print("-")

foreach i in 5..<1 { System.print(i) }
// Expect: 5, 4, 3, 2